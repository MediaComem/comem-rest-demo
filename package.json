{
  "name": "comem-rest-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "apidoc": "apidoc -i routes -o docs",
    "apidoc:watch": "onchange APIDOC.md routes/**/*.js -- npm run apidoc",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "concurrently \"npm run nodemon\" \"npm run apidoc:watch\"",
    "doctoc": "doctoc --github --notitle README.md",
    "nodemon": "DEBUG=demo:* AUTH_TOKEN=secret nodemon",
    "start": "node ./bin/www",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "test:ci": "cross-env NODE_ENV=test nyc mocha --forbid-only"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "format-link-header": "^3.1.0",
    "js-yaml": "^3.14.0",
    "mongoose": "^5.10.4",
    "morgan": "~1.10.0",
    "pug": "^3.0.1",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "apidoc": "^0.25.0",
    "chai": "^4.2.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "doctoc": "^1.4.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "onchange": "^7.0.2",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">= 14.0.0"
  }
}
