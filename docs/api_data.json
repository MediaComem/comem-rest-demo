[  {    "type": "post",    "url": "/api/characters",    "title": "Create a character in a movie",    "name": "CreateCharacter",    "group": "Character",    "version": "1.0.0",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "size": "3..30",            "optional": false,            "field": "name",            "description": "<p>The name of the character</p>"          },          {            "group": "Parameter",            "type": "String",            "allowedValues": [              "\"male,female\""            ],            "optional": false,            "field": "gender",            "description": "<p>The gender of the character</p>"          },          {            "group": "Parameter",            "type": "Number",            "size": "0..",            "optional": false,            "field": "age",            "description": "<p>The age of the character in years</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "movie",            "description": "<p>The ID of the movie in which the character appears</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "name",            "description": "<p>The name of the character</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "gender",            "description": "<p>The gender of the character</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "age",            "description": "<p>The age of the character in years</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "createdAt",            "description": "<p>The creation date of the character</p>"          }        ]      }    },    "error": {      "fields": {        "Error 422": [          {            "group": "Error 422",            "type": "Object",            "optional": false,            "field": "CharacterInvalid",            "description": "<p>Some of the character's properties are invalid</p>"          }        ]      },      "examples": [        {          "title": "CharacterInvalid",          "content": "HTTP/1.1 422 Not Found\nContent-Type: application/json\n\n{\n  \"message\": \"Character validation failed\",\n  \"errors\": {\n    \"name\": {\n      \"kind\": \"required\",\n      \"message\": \"Path `name` is required.\",\n      \"name\": \"ValidatorError\",\n      \"path\": \"name\",\n      \"properties\": {\n        \"message\": \"Path `{PATH}` is required.\",\n        \"path\": \"name\",\n        \"type\": \"required\"\n      }\n    }\n  }\n}",          "type": "json"        }      ]    },    "filename": "routes/characters.js",    "groupTitle": "Character"  }]
