  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "cross-env": "^10.0.0",
    "doctoc": "^2.2.0",
    "jest": "^30.1.3",
    "nodemon": "^3.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.0.3",
