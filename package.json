{
  "name": "comem-webdev-express-rest-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "apidoc": "apidoc -i routes -o docs",
    "apidoc:watch": "onchange APIDOC.md routes/**/*.js -- npm run apidoc",
    "dev": "concurrently \"npm run nodemon\" \"npm run apidoc:watch\"",
    "nodemon": "DEBUG=demo:* AUTH_TOKEN=secret nodemon",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "format-link-header": "^2.0.0",
    "jade": "~1.11.0",
    "mongoose": "^4.8.4",
    "morgan": "~1.7.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "concurrently": "^3.3.0",
    "nodemon": "^1.11.0",
    "onchange": "^3.2.1"
  },
  "engines": {
    "node": "12.x"
  }
}
